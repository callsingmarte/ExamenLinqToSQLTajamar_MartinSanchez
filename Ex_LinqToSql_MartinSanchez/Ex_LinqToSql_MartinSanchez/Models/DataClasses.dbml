<?xml version="1.0" encoding="utf-8"?><Database Name="GestionCursosDB" Class="DataClassesDataContext" xmlns="http://schemas.microsoft.com/linqtosql/dbml/2007">
  <Table Name="dbo.Cursos" Member="Cursos">
