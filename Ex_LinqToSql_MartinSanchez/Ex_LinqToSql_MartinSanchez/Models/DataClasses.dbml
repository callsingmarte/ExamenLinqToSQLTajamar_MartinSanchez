      <Column Name="Titulo" Type="System.String" DbType="NVarChar(100) NOT NULL" CanBeNull="false" />
      <Column Name="Descripcion" Type="System.String" DbType="NVarChar(255)" CanBeNull="true" />
      <Column Name="FechaInicio" Type="System.DateTime" DbType="Date" CanBeNull="true" />
      <Column Name="Duracion" Type="System.Int32" DbType="Int" CanBeNull="true" />
      <Association Name="Cursos_Inscripciones" Member="Inscripciones" ThisKey="CursoID" OtherKey="CursoID" Type="Inscripciones" />
    </Type>
  </Table>
  <Table Name="dbo.Usuarios" Member="Usuarios">
